
#Assigning elements to different lists#
list  = []
length = int(input("enter the length of the list :"))
i = 0
while (i <= length):
    number = int(input("enter a number"))
    list.append(number)
    i += 1
print("List is :" , list)


#Accessing elements froma tuple#
animals = ('lion' , 'tiger' , 'leopard')
print(animals[2])

#Deleting different dictionary elements#
animals = {
    'reptiles': 'snake',
    'mammals': 'whale',
    'amphibians': 'frogs'
}
del animals['reptiles']
print(animals)
